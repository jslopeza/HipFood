{
  "name": "ac-koa-hipchat-vagrant",
  "displayName": "HipChat Integration Example",
  "description": "Greets people when they type /hello in chat",
  "version": "0.1.0",
  "author": {
    "name": "Your Name",
    "url": "http://yourcompany.com"
  },
  "license": "Apache 2.0",
  "engines": {
    "node": "~0.11.13"
  },
  "scripts": {
    "web": "node --harmony web.js",
    "web-dev": "nodemon --harmony -e js,json,css,hbs web.js",
    "tunnel": "nodemon --harmony -e js,json,css,hbs tunnel.js"
  },
  "development": {
    "port": 3000
  },
  "production": {
    "localBaseUrl": "https://hipchat-integration-example.yourpaas.com",
    "redisEnv": "REDISCLOUD_URL",
    "port": "$PORT"
  },
  "dependencies": {
    "ac-koa": "^0.2.3",
    "ac-koa-hipchat": "^0.2.4",
    "ngrok": "^0.1.97"
  }
}
